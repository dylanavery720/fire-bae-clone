{
  "plugins": [
    "eslint-plugin-react"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
   "import/no-extraneous-dependencies": [error, { devDependencies: true }],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "react/jsx-uses-vars": [1],
    "react/react-in-jsx-scope": [1],
    "import/no-unresolved": [0],
    "import/extensions": [0],
    "arrow-parens": [0]
  }
}